{
  "manifest_version": 3,
  "name": "北科大教學評量小助手",
  "version": "1.0.3",
  "description": "Made by Kòng-uân Rider",
  "permissions": ["storage"], 
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
    },
        
  "content_scripts": [
    {
      "matches": [
        "https://isms-nagios.ntut.edu.tw/tgrade/PortalTGrade.jsp*",
        "https://isms-nagios.ntut.edu.tw/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}